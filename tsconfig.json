{
  "compilerOptions": {
    "types": ["node", "dotenv"],
    "target": "ESNext",
    "module": "ESNext",
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true
  },
  "typeRoots": [
    "./utils/global.d.ts", "./node_modules/@types"
  ],
  "include": [
    "src/**/*.ts", "src/config/config.ts"
, "src/server.mjs"  ],
  "exclude": [
    "node_modules"
  ]
}
